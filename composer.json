{
    "name": "test/website",
    "description": "A website that test Facade pattern",
    "scripts": {
      "serve": "php -S 127.0.0.1:8000 -t public",
	  "phpstan": "vendor/bin/phpstan analyse -vvv -l 9"
    },
    "autoload": {
      "psr-4": {
        "App\\": "app",
        "Framework\\": "framework"
      },
      "files": [
        "framework/function.php"
      ]
    },
    "config": {
      "process-timeout": 0
    },
    "require": {
        "symfony/console": "^7.1",
        "filp/whoops": "^2.15"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12"
    }
}
